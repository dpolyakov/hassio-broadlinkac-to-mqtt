{
    "name": "broadlink_ac_mqtt",
    "version": "0.0.1",
    "slug": "broadlink_ac_mqtt",
    "description": "Use your ZigBee devices without the vendor's bridge or gateway",
    "uart": true,
    "url": "https://github.com/dpolyakov/hassio-broadlinkac-to-mqtt/tree/master/broadlinkac-to-mqtt",
    "startup": "application",
    "services": [
      "mqtt:need"
    ],
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": false,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:zigbee",
    "map": [
      "share:rw",
      "config:rw"
    ],
    "options": {
      "data_path": "/config/zigbee2mqtt"
    },
    "image": "broadlinkac/broadlink_ac_mqtt-{arch}"
  }
